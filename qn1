import java.util.*;

public class Main {
    public static void main(String[] args) {
        int N = 2;
        int[] arr = {1, 2, 3, 2, 1, 4};
        int[] result = findSingleNumbers(arr, N);
        System.out.println(Arrays.toString(result));
    }

    public static int[] findSingleNumbers(int[] arr, int N) {
        Map<Integer, Integer> countMap = new HashMap<>();
        for (int num : arr) {
            countMap.put(num, countMap.getOrDefault(num, 0) + 1);
        }
        List<Integer> singleNumbers = new ArrayList<>();
        for (Map.Entry<Integer, Integer> entry : countMap.entrySet()) {
            if (entry.getValue() == 1) {
                singleNumbers.add(entry.getKey());
            }
        }
        Collections.sort(singleNumbers);
        return singleNumbers.stream().mapToInt(i -> i).toArray();
    }
}
